<% include partials/header %>
<div class="container-fluid">
    <% include partials/navigation %>
    <% include partials/signin %>
    <div class="row">
        <div class="col-md-12">
            <form class="form-inline">
                <div class="form-group">
                    <label for="name">Blockstack ID:</label>
                    <input type="text" name="blockstack_id" class="form-control" value="<%= query.blockstack_id %>">
                </div>
                <div class="form-group">
                    <label for="name">Equipment Serial No:</label>
                    <input type="text" name="equipment_serial" class="form-control" value="<%= query.equipment_serial %>">
                </div>
                <button type="submit" class="btn btn-primary">Search</button>
            </form>
            <hr/>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Model</th>
                            <th>Serial No</th>
                            <th>Brand</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody id="displayEquipment">
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<% include partials/footer %>